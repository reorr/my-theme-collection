!rofi.lines:                          5
!rofi.columns:                        1
!rofi.font:                           Iosevka 9
!rofi.bw:                             0
!rofi.location:                       0
!rofi.padding:                        30
!rofi.levenshtein-sort:               true
!rofi.case-sensitive:                 false
!rofi.fuzzy:                          false
!rofi.line-margin:                    5
!rofi.separator-style:                solid
